program
{
        integer x = 1;

        integer get_int() extern;

        integer print_int() extern;

        integer inc(integer x) {
                integer y = x + 1;
                return y;
        }

        void main() {
          integer y = 0;
          y = get_int();

          if (!(y > 100)) then {
            while (y < 100) {
              y = inc(y);
            }
          }


          print_int(y);
        }
}
