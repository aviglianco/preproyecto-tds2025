{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "parser",
      "camelcase": "Parser",
      "title": "Parser",
      "scope": "source.parser",
      "file-types": [
        "parser"
      ],
      "injection-regex": "^parser$",
      "class-name": "TreeSitterParser"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser grammar for tree-sitter",
    "authors": [
      {
        "name": "Agus"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-parser"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}